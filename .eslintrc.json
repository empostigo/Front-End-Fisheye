{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "quotes": ["warn", "double"],
    "semi": ["warn", "never"],
    "import/prefer-default-export": "off",
    "no-console": "off",
    "consistent-return": "warn",
    "import/extensions": ["error", "always"],
    "no-constructor-return": "off",
    "no-underscore-dangle": "off",
    "no-plusplus": "off"
  }
}
